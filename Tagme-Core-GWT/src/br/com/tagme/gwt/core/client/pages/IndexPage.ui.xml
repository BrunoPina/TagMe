<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:typeahead="urn:import:org.gwtbootstrap3.extras.typeahead.client.ui">
	
	<ui:with field="res" type="br.com.tagme.gwt.theme.tagme.client.CommonImagesFactory"/>
	
	<b:Row height="100%" addStyleNames="parent-center-vertical">
	
		<b:Column size="XS_12" height="100%" addStyleNames="center-vertical">
			<b:Image addStyleNames="center-block logo-indexpage" resource="{res.create.logosankhya}"></b:Image>
			<b:Row>
				<b:Column offset="XS_0,SM_1,MD_2,LG_3" size="XS_12,SM_10,MD_8,LG_6">
					<b:InputGroup addStyleNames="full-width">
						<typeahead:Typeahead addStyleNames="input-lg" ui:field="asyncTypeahead"/>
						<b:InputGroupButton>
							<b:Button addStyleNames="btn-search" type="DEFAULT" size="LARGE" icon="SEARCH" ui:field="botaoSearch"/>
						</b:InputGroupButton>
					</b:InputGroup>
				</b:Column>
			</b:Row>
			
		</b:Column>
		
	</b:Row>
	
</ui:UiBinder> 