<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	
	<b:Row>
		<b:Column size="XS_12"> 
			<b:Row addStyleNames="form-item">
				<b:Column size="XS_12, SM_6" offset="SM_3">
					<b:FormLabel addStyleNames="form-label">E-mail</b:FormLabel>
				</b:Column>
				<b:Column size="XS_12, SM_6" offset="SM_3">
					<b:TextBox addStyleNames="input-lg" ui:field="txbNome" validateOnBlur="true" />
				</b:Column>
			</b:Row>
			<b:Row addStyleNames="form-item">
				<b:Column size="XS_12, SM_6" offset="SM_3">
					<b:FormLabel addStyleNames="form-label">Celular</b:FormLabel>
				</b:Column>
				<b:Column size="XS_12, SM_4" offset="SM_3">
					<b:TextBox addStyleNames="input-lg" ui:field="txbTel" validateOnBlur="true" />
				</b:Column>
			</b:Row>				
		</b:Column>
	</b:Row>
	
</ui:UiBinder> 