<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"	
			xmlns:g="urn:import:com.google.gwt.user.client.ui" 
			xmlns:b="urn:import:org.gwtbootstrap3.client.ui">
	
	<b:Row>
		<b:Column size="XS_12">
			<b:Row>
				<b:Column addStyleNames="steps-btn-container left" size="XS_2">
					<b:Button ui:field="btnVoltar" text="Voltar" type="LINK" icon="ARROW_LEFT" />
				</b:Column>
				<b:Column size="XS_8">
					<b:PageHeader addStyleNames="page-header" ui:field="wizardTitle" />
				</b:Column>
				<b:Column addStyleNames="steps-btn-container right" size="XS_2">
					<b:Button ui:field="btnAvancar" type="LINK" text="Continuar"/>
				</b:Column>
			</b:Row>	
		</b:Column>
		<b:Column size="XS_12">
			<b:Row>
				<b:Column size="XS_12" ui:field="stepsIconContainer"  addStyleNames="steps-icons-container">
				
				</b:Column>
			</b:Row>
		</b:Column>
		<b:Column ui:field="stepsContainer" size="XS_12">
			
		</b:Column>
		<b:Column size="XS_12">
			<b:Row addStyleNames="form-item">
				<b:Column size="XS_12, SM_6" offset="SM_3">
					<b:Button ui:field="btnFinalizar" block="true" type="PRIMARY" text="Continuar"/>
				</b:Column>
			</b:Row>
		</b:Column>
	</b:Row>
</ui:UiBinder> 