<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
			 xmlns:g="urn:import:com.google.gwt.user.client.ui" 
			 xmlns:b="urn:import:org.gwtbootstrap3.client.ui" >
	
	<ui:with field="res" type="br.com.tagme.gwt.theme.tagme.client.CommonImagesFactory"/>
	
	<b:Column size="XS_6,SM_3,MD_1,LG_1">
		<b:ThumbnailLink ui:field="menuLink" addStyleNames="menu-link">
			<b:Tooltip ui:field="tooltip">
				<b:ThumbnailPanel ui:field="menuItem" addStyleNames="center-block iconed-menu">
					<b:Image ui:field="icon" addStyleNames="center-block"/>
					<b:Caption>
						<b:Heading ui:field="heading" size="H5"/>
					</b:Caption>
				</b:ThumbnailPanel>
			</b:Tooltip>
		</b:ThumbnailLink>
	</b:Column>
</ui:UiBinder> 